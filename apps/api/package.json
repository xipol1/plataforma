{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "db:migrate": "node ./scripts/db-migrate.mjs",
    "db:reset": "node ./scripts/db-reset.mjs",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio --schema prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.16.0",
    "argon2": "^0.44.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.13.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@prisma/adapter-pg": "^6.16.0",
    "@prisma/engines": "^5.22.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.10.2",
    "@types/pg": "^8.16.0",
    "prisma": "^6.16.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
